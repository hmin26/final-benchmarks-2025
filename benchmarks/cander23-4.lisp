(define (phantom-cascade x y z w)
  (let ((sum1 (+ (* x y) (* z w))))
    (let ((sum2 (+ (* w z) (* y x))))
      (let ((bundle (pair sum1 sum2)))
        (left (pair (left bundle) (right bundle)))))))

(define (unused-fanout x)
  (let ((stage1 (pair x (+ x x))))
    (let ((stage2 (pair (left stage1) (right stage1))))
      (right stage2))))

(let ((a 2))
